{
  "name": "inTrello",
  "version": "0.0.3",
  "description": "Add people data from LinkedIn profile to Trello boards as cards",
  "permissions": ["activeTab", "declarativeContent", "storage"],
  "background": {
    "scripts": ["jquery-3.3.1.min.js", "eventListener.js"],
    "persistent": false
  },
  "page_action": {
    "default_icon": {
      "16": "img/icon_16.png",
      "32": "img/icon_32.png",
      "48": "img/icon_64.png",
      "128": "img/icon_128.png"
    }
  },
  "icons": {
    "16": "img/icon_16.png",
    "32": "img/icon_32.png",
    "48": "img/icon_64.png",
    "128": "img/icon_128.png"
  },
  "manifest_version": 2
}
